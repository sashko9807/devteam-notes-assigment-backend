{
  "name": "devteam-notes-assigment-backend",
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "build": "tsc",
    "start:development": "yarn && NODE_ENV=development && nodemon src/index.ts",
    "start:staging": "yarn && yarn build && NODE_ENV=staging && node .build/dist/src/index.js",
    "start:production": "yarn && yarn build && NODE_ENV=production && node .build/dist/src/index.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "nodemon": "^3.1.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2"
  }
}
